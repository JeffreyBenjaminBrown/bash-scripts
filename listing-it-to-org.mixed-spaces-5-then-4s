sed -i "s/^                                                         [^ ] /*************** /g" $1.org
sed -i "s/^                                                     [^ ] /************** /g" $1.org
sed -i "s/^                                                 [^ ] /************* /g" $1.org
sed -i "s/^                                             [^ ] /************ /g" $1.org
sed -i "s/^                                         [^ ] /*********** /g" $1.org
sed -i "s/^                                     [^ ] /********** /g" $1.org
sed -i "s/^                                 [^ ] /********* /g" $1.org
sed -i "s/^                             [^ ] /******** /g" $1.org
sed -i "s/^                         [^ ] /******* /g" $1.org
sed -i "s/^                     [^ ] /****** /g" $1.org
